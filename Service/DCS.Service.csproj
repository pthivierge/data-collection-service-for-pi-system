  <PropertyGroup>
    <PostBuildEvent>::----------------------------------------
:: Copy to buid folder
::----------------------------------------
:copytoBuildFolder
SET output="..\..\..\Build"
if not exist "%25output%25" mkdir "%25output%25"
if not exist "%25output%25\bin" mkdir "%25output%25\bin"

robocopy "$(TargetDir)." "%25output%25\." $(TargetName).exe $(TargetName).pdb *.config *.ini *.ico *.html log4Net.xml settings.json /XO /NJH /NP /S /XD plugins
robocopy "$(TargetDir)." "%25output%25\bin\." *.dll /XO /NJH /NP /S /XD plugins
robocopy "$(TargetDir)\WebConfig" "%25output%25\webconfig" *.* /XO /NJH /NP /S

if errorlevel 4 goto BuildEventFailed
if errorlevel 0 goto end

:BuildEventFailed echo FILECOPY for $(ProjectName) FAILED
exit 1
:end echo FILECOPY for $(ProjectName) COMPLETED OK
exit 0</PostBuildEvent>
