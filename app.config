<?xml version="1.0"?>
<configuration>

  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="WSR.Settings.Fitbit" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
      <section name="WSR.Settings.General" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
      <section name="WSR.Settings.Advanced" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <applicationSettings>
    <WSR.Settings.Fitbit>
      <setting name="AFTemplateForFitBit" serializeAs="String">
        <value>FitBit Device Template</value>
      </setting>
      <setting name="AppConsumerKey" serializeAs="String">
        <value>YOU_CONSUMER_KEY_HERE!</value>
      </setting>
      <setting name="AppConsumerSecret" serializeAs="String">
        <value>YOU_CONSUMER_SECRET_HERE</value>
      </setting>
      <setting name="Readers_InitdaysOfHistory" serializeAs="String">
        <value>180</value>
      </setting>
      <setting name="Readers_NormalRunDaysOfHistory" serializeAs="String">
        <value>15</value>
      </setting>
    </WSR.Settings.Fitbit>
    <WSR.Settings.General>
      <setting name="cronPeriod_CollectData" serializeAs="String">
        <value>0/1 * * * * ?</value>
      </setting>
      <setting name="cronPeriod_Refresh" serializeAs="String">
        <value>0/30 * * * * ?</value>
      </setting>
      <setting name="cronPeriod_Write" serializeAs="String">
        <value>0/5 * * * * ?</value>
      </setting>
    </WSR.Settings.General>
    <WSR.Settings.Advanced>
      <setting name="StartDebuggerOnStart" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="NoConnectionSImulateData" serializeAs="String">
        <value>False</value>
      </setting>
    </WSR.Settings.Advanced>
   
  </applicationSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="bin"/>
    </assemblyBinding>
  </runtime>
</configuration>
